{
  "compilerOptions": {
    "target": "es2022",
    "useDefineForClassFields": false,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitOverride": true,
    "noPropertyAccessFromIndexSignature": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "paths": {
      "@env": ["apps/manager/src/environments/environment"],
      "@models": ["apps/manager/src/app/shared/models"],
      "@store*": ["apps/manager/src/app/store/"],
      "@nx/shared/types": ["libs/shared/types/src/index.ts"],
      "@nx/shared/components": ["libs/shared/components/src/index.ts"],
      "@nx/shared/store": ["libs/shared/store/src/index.ts"],
      "@nx/news": ["libs/news/src/index.ts"],
      "@nx/shared/services": ["libs/shared/services/src/index.ts"],
      "@nx/shared/utils": ["libs/shared/utils/src/index.ts"]
    },
  },
  "files": [],
  "include": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.spec.json"
    },
    {
      "path": "./tsconfig.editor.json"
    }
  ],
  "extends": "../../tsconfig.base.json",
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}
